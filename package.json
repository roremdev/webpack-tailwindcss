{
  "name": "webpack-tailwindcss",
  "version": "2.1.0",
  "description": "Webpack for Pug, SASS + Tailwind",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "dev": "npm run build && live-server --port=8080 --host=localhost --open=./dist",
    "start": "webpack serve --config webpack.config.dev.babel.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "Webpack",
    "JavaScript",
    "Pug",
    "SASS",
    "Tailwind",
    "Storybook"
  ],
  "author": "@roremDev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roremdev/webpack-tailwindcss/issues"
  },
  "homepage": "https://github.com/roremdev/webpack-tailwindcss",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/register": "^7.13.16",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/builder-webpack5": "^6.2.9",
    "@storybook/html": "^6.2.9",
    "@storybook/preset-scss": "^1.0.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.6.0",
    "postcss": "^8.3.0",
    "postcss-loader": "^5.3.0",
    "postcss-nested": "^5.0.5",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "pug-runtime": "^2.0.5",
    "sass": "^1.34.1",
    "sass-loader": "^12.0.0",
    "storypug": "^1.0.0-rc.8",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.1.4",
    "terser-webpack-plugin": "^5.1.3",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
